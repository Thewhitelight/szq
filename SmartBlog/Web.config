<configuration>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
      <httpRuntime requestValidationMode="2.0"/>
    </system.web>
  <connectionStrings>
    <add name="connStr" connectionString="server=.;database=SmartBlog;user id=sa;password=123456"/>
  </connectionStrings>

  <appSettings>
    <!--DbHelperProvider:对应命名空间-->
    <add key="DbHelperProvider" value="System.Data.SqlClient"/>
    <!--connStr:连接字符串-->
    <add key="connStr" value="server=.;database=SmartBlog;user id=sa;password=123456"/>
  </appSettings>
 
</configuration>
